{
    "root": true,
    "env": {
        "es6": true
    },
    "plugins": [
        "prettier"
    ],
    "ignorePatterns": ["**/e2e/*.ts"],
    "extends": [
        "plugin:vue/vue3-essential",
        "eslint:recommended"
    ],
    "parserOptions": {
        "parser": "@typescript-eslint/parser"
    },
    "rules": {
        "vue/multi-word-component-names": "off"
    }
}